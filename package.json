{
    "name": "byte-coder-ai-agent",
    "displayName": "Byte Coder - Agentic AI Coding Agent",
    "description": "The Top-Rated Agentic AI for VS Code. Autonomous Coding, Deep Debugging, and Self-Healing Plans. Better than Copilot.",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#050608",
        "theme": "dark"
    },
    "publisher": "uthakkan",
    "author": {
        "name": "UTHAKKAN (Ajmal U K)",
        "email": "contact.uthakkan@gmail.com",
        "url": "https://uthakkan.pythonanywhere.com"
    },
    "homepage": "https://uthakkan.pythonanywhere.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/ajmal-uk/byte-coder-ai-agent"
    },
    "version": "1.3.0",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Programming Languages",
        "Machine Learning",
        "Education",
        "Snippets"
    ],
    "keywords": [
        "agentic",
        "autonomous",
        "ai agent",
        "cursor",
        "devin",
        "cline",
        "copilot",
        "gpt-4",
        "deepseek",
        "claude",
        "pair programmer",
        "debugging",
        "automation",
        "python",
        "javascript",
        "typescript",
        "chatgpt",
        "free ai"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "byte-ai-sidebar",
                    "title": "Byte AI",
                    "icon": "$(hubot)"
                }
            ]
        },
        "views": {
            "byte-ai-sidebar": [
                {
                    "type": "webview",
                    "id": "byteAI.chatView",
                    "name": "Chat",
                    "icon": "images/logo.png"
                }
            ]
        },
        "commands": [
            {
                "command": "byteAI.clearChat",
                "title": "Clear Chat"
            },
            {
                "command": "byteAI.explainCode",
                "title": "Byte AI: Explain Selection"
            },
            {
                "command": "byteAI.fixCode",
                "title": "Byte AI: Fix Bugs"
            },
            {
                "command": "byteAI.refactorCode",
                "title": "Byte AI: Refactor Code"
            },
            {
                "command": "byteAI.generateTest",
                "title": "Byte AI: Generate Tests"
            },
            {
                "command": "byteAI.generateDocs",
                "title": "Byte AI: Generate Docs"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "byteAI.explainCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.fixCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.refactorCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateTest",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateDocs",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.85.0",
        "@types/ws": "^8.18.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "uuid": "^9.0.1",
        "ws": "^8.16.0"
    }
}